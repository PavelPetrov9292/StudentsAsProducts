#################################################################
#    Rules
#################################################################

:e rdf:type swrl:Variable .

:o rdf:type swrl:Variable .

:p rdf:type swrl:Variable .

:s rdf:type swrl:Variable .

:pos rdf:type swrl:Variable .

:paths rdf:type swrl:Variable .

:m rdf:type swrl:Variable .

:i rdf:type swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "" ;
   rdfs:label "Правило за съответствие между желана позиция и студентски профил" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :Employer ;
                           swrl:argument1 :e
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate base:postsJob ;
                                      swrl:argument1 :e ;
                                      swrl:argument2 :o
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate base:requiresProfile ;
                                                 swrl:argument1 :o ;
                                                 swrl:argument2 :p
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:ClassAtom ;
                                                            swrl:classPredicate :Student ;
                                                            swrl:argument1 :s
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                       swrl:propertyPredicate base:hasProfile ;
                                                                       swrl:argument1 :s ;
                                                                       swrl:argument2 :p
                                                                     ] ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate base:isMatchedWith ;
                           swrl:argument1 :s ;
                           swrl:argument2 :o
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "" ;
   rdfs:label "Правило за съответствие между желани позиции и свободни места" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :Student ;
                           swrl:argument1 :s
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                      swrl:propertyPredicate :desiredPositions ;
                                      swrl:argument1 :s ;
                                      swrl:argument2 :pos
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate :Employer ;
                                                 swrl:argument1 :e
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                                            swrl:propertyPredicate :careerPaths ;
                                                            swrl:argument1 :e ;
                                                            swrl:argument2 :paths
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:BuiltinAtom ;
                                                                       swrl:builtin swrlb:containsIgnoreCase ;
                                                                       swrl:arguments ( :paths
                                                                                        :pos
                                                                                      )
                                                                     ] ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :matchingPosition ;
                           swrl:argument1 :s ;
                           swrl:argument2 :e
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "" ;
   rdfs:label "Правило за намиране на подходящи стажове според специалността" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :Student ;
                           swrl:argument1 :s
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                      swrl:propertyPredicate :major ;
                                      swrl:argument1 :s ;
                                      swrl:argument2 :m
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate :Employer ;
                                                 swrl:argument1 :e
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                                            swrl:propertyPredicate :industry ;
                                                            swrl:argument1 :e ;
                                                            swrl:argument2 :i
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:BuiltinAtom ;
                                                                       swrl:builtin swrlb:containsIgnoreCase ;
                                                                       swrl:arguments ( :i
                                                                                        :m
                                                                                      )
                                                                     ] ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :suitableInternship ;
                           swrl:argument1 :s ;
                           swrl:argument2 :e
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .


